<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('partials/head'); %> 
    <title>Manager Dashboard</title>
    <link rel="icon" href="images/companyicon.png">
    <link rel="stylesheet" href="/stylesheets/landingpage.css">
    <link rel="stylesheet" href="/stylesheets/navbar.css">
    <link rel="stylesheet" href="/stylesheets/dashboards.css">

</head>

<body class="body-styles">

    <div class="navbar sticky-top">

        <div class="logo">
            <img src="/images/logo.png" alt="logo">
        </div>

        <div class="navlinks">
            <a href="">Manager Dashboard</a>
            <a href="./logout">Log Out (<%= type %> )</a>
        </div>
    </div>
    <div class="content">
        <div class="row">
            <div class="col-md-6">
                <div class="row box" id="registerCustomer">
                    <a href="/manager/customercreate">Register New customer</a> 
                    <%- include('partials/custregister'); %>
                </div>
                <div class="row box" id="employeeCompare">
                    <div class="sub-box">
                        <%- include('partials/empperformance', {empPerformance : empPerformance}) %>
                    </div>
                </div>  
                
            </div>
            <div class="col box" id="registerEmployee">
                <!-- <a href="/manager/employeecreate">Register New Employee</a> -->
                <%- include('partials/empregister') %>
            </div>
        </div>
    </div>
    <%- include('partials/footerscripts') %>

</body>

</html>