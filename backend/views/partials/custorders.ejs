<h3 class="headings">
    Orders
</h2>
<div class="table-responsive">
    <table class="table">
        <tr>
            <th>Order ID</th>
            <th>Product</th> 
            <th>Quantity</th>
            <th>Status</th>
        </tr>
        <% custOrders.forEach(ord => { %>
            <tr>
                <td>#<%= ord.order_id %></td>
                <td><%= ord.name %></td>
                <td><%= ord.qty %></td>
                <td><%= ord.status %></td>
            </tr>
        <% }); %>
    </table>
</div>