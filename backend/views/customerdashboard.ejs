<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('partials/head') %> 
    <title>Customer Dashboard</title>
    <link rel="icon" href="images/companyicon.png">
    <link rel="stylesheet" href="/stylesheets/landingpage.css">
    <link rel="stylesheet" href="/stylesheets/navbar.css">
    <link rel="stylesheet" href="/stylesheets/dashboards.css">

</head>

<body class="body-styles">

    <div class="navbar sticky-top">

        <div class="logo">
            <img src="/images/logo.png" alt="logo">
        </div>

        <div class="navlinks">
            <a href="/">Home</a>
            <a href="#">Customer Dashboard</a>
            <a href="/logout">Log Out (<%= type %> )</a>
        </div>
    </div>
    <div class="content">
        <div class="row">
            <div class="col">
                <div class="row box">
                    <div class="cust-details">
    
                    </div>
                    <div class="order-links">
                        
                    </div>
                </div>
            </div>
            <div class="col box">
                <div class="your-orders">
                    <%- include('partials/custorders', {custOrders : custOrders}) %> 
                </div>
            </div>
        </div>
        
    </div>
    <%- include('partials/footerscripts') %> 

</body>

</html>